{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "shopping-cart/*": ["./src/app/*"],
      "shopping-cart-api/*": ["public/*"]
    },
    "types": ["svelte", "node"],
    "resolveJsonModule": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules/*", "__sapper__/*", "public/build/*"],
}